{
  "name": "crisjumbo-component",
  "description": "Component library with React and TailwindCSS designed by Cris Jumbo",
  "author": "Cris Jumbo (https://crisjumbo.me/)",
  "version": "0.2.2",
  "keywords": [
    "react",
    "tailwindcss",
    "component-library"
  ],
  "homepage": "https://github.com/crisjumbo/crisjumbo-components#readme",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
